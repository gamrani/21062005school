package com.school.api.entities;

import javax.persistence.Column;

@Entity
public class Student extends User {

    @Column
	private String cne;

	public Student() {
		super();
	}

	public Student(String cne) {
		super();
		this.cne = cne;
	}

	public String getCne() {
		return cne;
	}

	public void setCne(String cne) {
		this.cne = cne;
	}
	
	
}
